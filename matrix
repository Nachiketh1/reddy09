3	
4	int main()
5	{
6	    int i, j, count;
7	
8	    int arr[3][3];
9	
10	    count = 0;
11	    for (i=0;i<3;i++)
12	    {
13	      for (j=0;j<3;j++)
14	      {
15	         scanf("%d",&arr[i][j]);
16	      }
17	    }
18	
19	    for (i=0;i<3;i++)
20	    {
21	      for (j=0;j<3;j++)
22	      {
23	         printf("%d",arr[i][j]);
24	      }
25	    }
26	     for(i=0;i<3;i++)
27	     {
